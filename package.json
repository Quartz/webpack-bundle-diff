{
  "name": "webpack-bundle-diff",
  "version": "0.1.0",
  "description": "Understand changes in webpack bundle size",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "bin": {
    "webpack-bundle-diff": "./bin/webpack-bundle-diff",
    "wbd": "./bin/webpack-bundle-diff"
  },
  "scripts": {
    "build": "tsc",
    "debug": "yarn build && node --inspect-brk --max-old-space-size=12288 lib/cli.js data sampledata/stats.json -o sampledata/bundledata.json",
    "test": "jest"
  },
  "author": "Scott Mikula <mikula@gmail.com>",
  "dependencies": {
    "@types/event-stream": "^3.3.34",
    "JSONStream": "^1.3.5",
    "commander": "^2.19.0",
    "event-stream": "^4.0.1"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/jest": "^23.3.11",
    "@types/node": "^10.12.18",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "typescript": "~3.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smikula/webpack-bundle-diff.git"
  },
  "license": "MIT"
}
